					"script": {
						"exec": [
							"let authResponse = pm.response.json().AuthenticationResult;",
							"",
							"if(authResponse) {",
							"    pm.collectionVariables.set('AccessToken', authResponse.AccessToken);",
							"    pm.collectionVariables.set('RefreshToken', authResponse.RefreshToken);",
